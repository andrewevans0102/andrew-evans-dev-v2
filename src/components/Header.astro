  <style>
    header {
      margin: 0;
      padding: 0 1em;
      background: white;
      box-shadow: 0 2px 8px rgba(var(--black), 5%);
    }
    h2 {
      margin: 0;
      font-size: 1em;
    }

    h2 a,
    h2 a.active {
      text-decoration: none;
    }
    nav {
      display: flex;
      align-items: center;
      justify-content: space-between;
      z-index: 1000;
    }
    nav a {
      padding: 1em 0.5em;
      color: var(--black);
      border-bottom: 4px solid transparent;
      text-decoration: none;
    }
    nav a.active {
      text-decoration: none;
      /* border-bottom-color: var(--accent); */
    }
    .social-links,
    .social-links a {
      display: flex;
    }

    :global(.dark) nav {
      background-color: var(--dark-navbar);
      color: var(--dark-color);
    }

    :global(.dark) header {
      background-color: var(--dark-navbar);
      color: var(--dark-color);
    }

    img {
      height: 40px;
      border-radius: none;
    }

    /* @media (max-width: 720px) {
		.social-links {
			display: none;
		}
	} */
    @media (max-width: 720px) {
      nav {
        display: flex;
        align-items: center;
        justify-content: space-between;
        z-index: 1000;
        flex-direction: column;
        margin: 0;
      }
      /* 
		nav a {
			padding: 1em 0.5em;
			padding: 0;
			color: var(--black);
			border-bottom: 4px solid transparent;
			text-decoration: none;
		}
		nav a.active {
			text-decoration: none;
			border-bottom-color: var(--accent);
		} */
    }
  </style>
</header>
